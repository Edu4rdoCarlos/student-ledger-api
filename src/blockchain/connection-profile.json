    "peer0.aluno.ifal.local": {
      "url": "grpcs://localhost:9051",
      "tlsCACerts": {
        "path": "organizations/peerOrganizations/aluno.ifal.local/peers/peer0.aluno.ifal.local/tls/ca.crt"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer0.aluno.ifal.local"
      }
