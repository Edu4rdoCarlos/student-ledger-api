{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "blockchain/certs/**/*",
        "outDir": "dist"
      },
      {
        "include": "blockchain/connection-profile.json",
        "outDir": "dist"
      }
    ],
    "watchAssets": true
  }
}
