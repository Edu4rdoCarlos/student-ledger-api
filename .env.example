# Application
NODE_ENV=development
PORT=3000

# PostgreSQL
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/student_ledger?schema=public"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="1d"

# Hyperledger Fabric
# Organização padrão para conectar (coordenacao, orientador ou aluno)
FABRIC_DEFAULT_ORG="coordenacao"

# Canal do Hyperledger Fabric
FABRIC_CHANNEL="studentchannel"

# Chaincode name
FABRIC_CHAINCODE="student-ledger"

# IPFS
IPFS_API_URL="http://localhost:5001"

# Redis
REDIS_HOST="localhost"
REDIS_PORT=6379
